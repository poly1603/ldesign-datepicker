{
  "name": "@ldesign/datepicker-vue",
  "version": "1.0.0",
  "description": "LDesign DatePicker Vue Component",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "@ldesign/datepicker-core/styles"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "datepicker",
    "vue",
    "vue3",
    "calendar",
    "timepicker"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "@ldesign/datepicker-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  }
}
